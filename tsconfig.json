{
  "compilerOptions": {
    // "outDir": "dist",
    "allowJs": true,
    "target": "es6",
    "noEmit": true,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "jsxImportSource": "@emotion/react",
    "lib": [
        "es6",
        "dom"
    ],
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build", "scripts"]
}